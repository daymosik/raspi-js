{
  "compilerOptions": {
    "lib": [
      "dom",
      "es2017"
    ],
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "strictNullChecks": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@assets/*": [ "src/assets/*" ],
      "@components/*": [ "src/components/*" ],
      "@modules/*": [ "src/modules/*" ],
      "@services/*": [ "src/services/*" ],
      "@functions/*": [ "src/functions/*" ],
      "@classes/*": [ "src/classes/*" ]
    }
  }
}
